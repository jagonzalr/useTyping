{
  "name": "use-typing",
  "version": "1.0.1",
  "description": "Simulate typing with React hooks.",
  "main": "lib/index.js",
  "files": [
    "lib/index.js"
  ],
  "scripts": {
    "start": "NODE_ENV=development BABEL_ENV=development node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.demo.js --env=dev --mode development --host 127.0.0.1 --port 3000 --progress --inline --open",
    "build": "webpack --config webpack.config.js --mode production",
    "prepare": "echo 'Building useTyping' && npm run build",
    "lint": "eslint src --ext .js,.jsx --fix && eslint demo --ext .js,.jsx --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jagonzalr/use-typing.git"
  },
  "keywords": [
    "Typing",
    "React",
    "Hooks"
  ],
  "author": "Jose Antonio Gonzalez Rodriguez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jagonzalr/use-typing/issues"
  },
  "homepage": "https://github.com/jagonzalr/use-typing#readme",
  "peerDependencies": {
    "react": "^16.8.x"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "core-js": "^3.6.5",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "node-sass": "^4.14.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react": "^16.13.1",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "sass-loader": "^8.0.2",
    "tailwindcss": "^1.3.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "npm run --silent lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "dependencies": {
    "style-loader": "^1.2.1"
  }
}
